<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_home"
    android:id="@+id/mobile_navigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dicodingevent.ui.fragment.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.example.dicodingevent.ui.fragment.UpcomingEventFragment"
        android:label="Upcoming"
        tools:layout="@layout/fragment_upcoming_event" />

    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.example.dicodingevent.ui.fragment.FinishedEventFragment"
        android:label="Finished"
        tools:layout="@layout/fragment_finished_event" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.dicodingevent.ui.fragment.FavoriteFragment"
        android:label="Favorite"
        tools:layout="@layout/fragment_favorite" />

</navigation>